{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "id",
    "nome",
    "cpf",
    "email",
    "telefone",
    "endereco"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "nome": {
      "type": "string"
    },
    "cpf": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "telefone": {
      "type": "string"
    },
    "endereco": {
      "type": "object",
      "required": [
        "id",
        "logradouro",
        "rua",
        "numero",
        "bairro",
        "cidade",
        "estado"
      ],
      "properties": {
        "id": {
          "type": "integer"
        },
        "logradouro": {
          "type": "string"
        },
        "rua": {
          "type": "string"
        },
        "numero": {
          "type": "integer"
        },
        "bairro": {
          "type": "string"
        },
        "cidade": {
          "type": "string"
        },
        "estado": {
          "type": "string"
        }
      }
    }
  }
}